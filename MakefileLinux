CC=g++
Objects=UI.o main.o Chess.o MyBot.o NetPlayer.o Chessboard.o ChessController.o
Include=-Isfml/include
Lib=-Lsfml/lib -lsfml-window -lsfml-system -lsfml-network -lsfml-graphics -pthread
Flags=-O3

all: $(Objects)
	$(CC) $(Objects) -o build/Chess-app  $(Flags) $(Lib)

clear:
	rm $(Objects)

%.o: sourse/%.cpp
	$(CC) -c $< -o $@ $(Flags) $(Include)
